    POPUP "POPUP"
    BEGIN
        MENUITEM "Faites store &standard",      IDM_STORE_MAKEDEFAULT
        POPUP "Ajouter &fichiers"
        BEGIN
            MENUITEM "&Importer dossier...",        IDM_STORE_IMPORTFOLDER
            MENUITEM "L'assistant de &migration",   IDM_STORE_MIGRATIONWIZARD
        END
        MENUITEM SEPARATOR
        MENUITEM "Créer r&accourci",            IDM_STORE_SHORTCUT
        MENUITEM "&Effacer",                    IDM_STORE_DELETE
        MENUITEM "&Renommer",                   IDM_STORE_RENAME
        MENUITEM SEPARATOR
        MENUITEM "&Propriétés",                 IDM_STORE_PROPERTIES
    END
END

IDM_STORES MENU 
BEGIN
    POPUP "POPUP"
    BEGIN
        MENUITEM "Créez un store &nouveau...",  IDM_STORES_CREATENEW
    END
END

IDM_CONDITION MENU 
BEGIN
    POPUP "POPUP"
    BEGIN
        MENUITEM "&Modifier...",                IDM_CONDITION_EDIT
        MENUITEM SEPARATOR
        MENUITEM "&Effacer",                    IDM_CONDITION_DELETE
    END
END

IDM_UPDATE MENU 
BEGIN
    POPUP "POPUP"
    BEGIN
        MENUITEM "&Restaurer",                  IDM_UPDATE_RESTORE
    END
END


